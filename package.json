{
  "name": "summit-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config webpack.config.js",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.js",
    "lint": "tslint ./src/**/*.tsx",
    "doc": "del ./docs && esdoc"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tslint ./src/**/*.tsx",
      "pre-push": "tslint ./src/**/*.tsx"
    }
  },
  "standard": {
    "globals": [
      "console",
      "Error",
      "config",
      "$",
      "antd",
      "woff"
    ],
    "properties": false,
    "parser": "babel-eslint",
    "ignore": [
      "./public/dist/",
      "./build/"
    ]
  },
  "browserslist": [
    ">5%",
    "last 4 versions",
    "Firefox ESR",
    "not ie < 9"
  ],
  "dependencies": {
    "@types/react-router-dom": "^4.2.7",
    "antd": "^3.6.1",
    "axios": "^0.18.0",
    "echarts": "^4.1.0",
    "echarts-for-react": "^2.0.13",
    "font-awesome": "^4.7.0",
    "maptalks": "^0.40.3",
    "pubsub-js": "^1.6.0",
    "react": "^16.4.0",
    "react-csv-reader": "^0.3.2",
    "react-dom": "^16.4.0",
    "react-icons": "^2.2.7",
    "react-router-dom": "^4.3.1"
  },
  "devDependencies": {
    "@types/jest": "^23.0.0",
    "@types/node": "^10.3.2",
    "@types/react": "^16.3.17",
    "@types/react-dom": "^16.0.6",
    "@types/react-icons": "^2.2.5",
    "autoprefixer": "^8.6.2",
    "awesome-typescript-loader": "^5.0.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-import": "^1.7.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "clean-webpack-plugin": "^0.1.19",
    "console-polyfill": "^0.3.0",
    "cross-env": "^5.2.0",
    "css-hot-loader": "^1.3.9",
    "css-loader": "^0.28.11",
    "cssnano": "^3.10.0",
    "del-cli": "^1.1.0",
    "esdoc": "^1.1.0",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint-plugin-typescript": "^0.12.0",
    "file-loader": "^1.1.11",
    "happypack": "^5.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.0.0-rc.9",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.9.0",
    "postcss-calc": "^6.0.1",
    "postcss-easy-import": "^3.0.0",
    "postcss-flexbugs-fixes": "^3.3.1",
    "postcss-loader": "^2.1.5",
    "precss": "^3.1.2",
    "progress-bar-webpack-plugin": "^1.11.0",
    "sass-loader": "^7.0.3",
    "source-map-loader": "^0.2.3",
    "standard": "^11.0.1",
    "style-loader": "^0.21.0",
    "tslint": "^5.10.0",
